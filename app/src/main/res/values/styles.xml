<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RadioButtonStyle" parent="Theme.AppCompat.Light">
        <item name="colorAccent">#4634A7</item>
        <item name="android:textColorPrimary">#4634A7</item>
    </style>

    <style name="Primary" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">20dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">#4634A7</item>
        <item name="android:elevation">16dp</item>
        <item name="android:letterSpacing">0</item>
        <item name="fontFamily">@font/poppins_medium</item>
    </style>

    <style name="Secondary" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">20dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#4634A7</item>
        <item name="android:backgroundTint">#FFFFFF</item>
        <item name="android:elevation">16dp</item>
        <item name="android:letterSpacing">0</item>
        <item name="fontFamily">@font/poppins_medium</item>

    </style>
</resources>
